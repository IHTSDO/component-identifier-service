<p>Namespace: {{item.namespace}}</p>
<p>Email: {{item.email}}</p>
<p>Organization Name: {{item.organizationName}}</p>
<table class="table table-bordered table-hover">
    <thead>
    <tr>
        <th class="text-center" colspan="2">Partitions</th>
    </tr>
    <tr>
        <th>Partition</th>
        <th>Sequence</th>
    </tr>
    </thead>
    <tbody>
    {{#each item.partitions}}
        <tr>
            <td>{{partitionId}}</td>
            <td>{{sequence}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>